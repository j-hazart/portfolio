---
import Skill from "./Skill.astro";
const {type, technologies} = Astro.props;
---

<div class="flex flex-col items-center gap-4 flex-1">
    <h3 class="capitalize text-2xl font-bold">{type}</h3>
    <ul class="flex flex-col gap-4">
        {
            technologies.map((techno) => <Skill techno={techno} />)
        }
    </ul>
</div>